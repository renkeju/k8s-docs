{
    "title": "Kubernetes Documentation",
    "author": "renkeju",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "Home": "https://kubernetes.renkeju.mobi"
        },
        "gitbook": false,
        "sharing": {
            "twitter": false,
            "facebook": false,
            "all": false
        }
    },
    "plugins": [
        "-lunr",
        "-search",
        "search-plus",
        "github",
        "todo",
        "terminal",
        "anchors",
        "edit-link",
        "image-captions",
        "popup",
        "custom-favicon"
    ],
    "pluginsConfig": {
        "favicon": "images/favicon.png",
        "github": {
            "url": "https://github.com/renkeju/kubernetes"
        },
        "terminal": {
            "copyButtons": true,
            "fade": true,
            "style": "flat"
        },
        "edit-link": {
            "base": "https://github.com/renkeju/kubernetes/edit/master",
            "label": "Edit This Page"
        },
        "image-captions": {
            "caption": "å›¾ _PAGE_LEVEL_ - _CAPTION_ ",
            "images": {
                "1.1.1": {
                    "skip": true
                },
                "1.1.2": {
                    "skip": true
                }
            }
        }
    }
}

