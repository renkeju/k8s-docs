{
    "title": "Kubernetes Documentation",
    "author": "renkeju",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "Home": "https://kubernetes.renkeju.mobi"
        }
    },
    "plugins": [
        "-lunr",
        "-search",
        "search-plus",
        "github",
        "todo",
        "terminal",
        "anchors",
        "edit-link",
        "custom-favicon"
    ],
    "pluginsConfig": {
        "favicon": "images/favicon.png",
        "github": {
            "url": "https://github.com/renkeju"
        },
        "terminal": {
            "copyButtons": true,
            "fade": true,
            "style": "flat"
        },
        "anchor-navigation-ex": {
            "isRewritePageTitle": true,
            "isShowTocTitleIcon": true,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
        "edit-link": {
            "base": "https://github.com/renkeju/kubernetes/edit/master",
            "label": "Edit This Page"
        }
    }
}

