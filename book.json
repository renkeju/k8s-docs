{
    "title": "Kubernetes Documentation",
    "author": "renkeju",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "Home": "https://kubernetes.renkeju.mobi"
        }
    },
    "plugins": [
        "-lunr",
        "-search",
        "search-plus",
        "github",
        "todo",
        "terminal",
        "custom-favicon"
    ],
    "pluginsConfig": {
        "favicon": "images/favicon.png",
        "github": {
            "url": "https://github.com/renkeju"
        },
        "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
        }
    }
}

